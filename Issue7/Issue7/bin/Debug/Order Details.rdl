  </DataSources>
  <DataSets>
    <DataSet Name="SalesDetail">
      <Query>
        <DataSourceName>dsContosoDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SubCategoryKey">
            <Value>=Parameters!SubCategoryKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  p.ProductName,fs.UnitPrice,fs.SalesQuantity,d.FullDateLabel AS OrderDate FROM 
dbo.FactSales fs INNER JOIN 
dbo.DimProduct p ON p.ProductKey = fs.ProductKey INNER JOIN dbo.DimDate d
ON d.Datekey = fs.DateKey
WHERE p.ProductSubcategoryKey= @SubCategoryKey
ORDER BY p.ProductName ASC,d.FullDateLabel DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UnitPrice">
          <DataField>UnitPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SalesQuantity">
          <DataField>SalesQuantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SubCategory">
      <Query>
        <DataSourceName>dsContosoDW</DataSourceName>
        <CommandText>SELECT DISTINCT ProductSubcategoryKey,ProductSubcategoryName FROM dbo.DimProductSubcategory ORDER BY ProductSubcategoryName</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductSubcategoryKey">
          <DataField>ProductSubcategoryKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductSubcategoryName">
          <DataField>ProductSubcategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>

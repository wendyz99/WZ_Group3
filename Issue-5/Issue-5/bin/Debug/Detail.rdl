  </DataSources>
  <DataSets>
    <DataSet Name="dsDetail">
      <Query>
        <DataSourceName>MainDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StoreName">
            <Value>=Parameters!StoreName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ds.StoreName, dg.ContinentName, dg.RegionCountryName, ds.AddressLine1, ds.ZipCode, dg.CityName, dg.StateProvinceName, de.FirstName, de.LastName, 
                         de.Title, ds.StoreType, ds.Status, ds.StorePhone, ds.StoreFax, ds.SellingAreaSize, ds.EmployeeCount
FROM            DimStore AS ds INNER JOIN
                         DimGeography AS dg ON ds.GeographyKey = dg.GeographyKey INNER JOIN
                         DimEmployee AS de ON ds.StoreManager = de.EmployeeKey
WHERE        (ds.StoreName = @StoreName)</CommandText>
      </Query>

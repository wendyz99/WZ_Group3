  <DataSets>
    <DataSet Name="Retail">
      <Query>
        <DataSourceName>Contoso</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CalendarYear">
            <Value>=Parameters!CalendarYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        fs.channelKey, dc.ChannelName, SUM(fs.SalesAmount) AS Sum_SalesAmount, SUM(fs.SalesQuantity) AS Sum_SalesQuantity, SUM(fs.TotalCost) AS Sum_TotalCost, 
                         dd.CalendarYear
FROM            FactSales AS fs INNER JOIN
                         DimChannel AS dc ON dc.ChannelKey = fs.channelKey INNER JOIN
                         DimDate AS dd ON dd.Datekey = fs.DateKey
where CalendarYear in (@CalendarYear)
GROUP BY fs.channelKey, dc.ChannelName, dd.CalendarYear
</CommandText>
      </Query>

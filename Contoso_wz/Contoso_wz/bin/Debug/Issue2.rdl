  <DataSets>
    <DataSet Name="Detail">
      <Query>
        <DataSourceName>issue2</DataSourceName>
        <CommandText>SELECT        fs.ProductKey, dp.ProductName, SUM(fs.SalesAmount) AS Net_Revenue, SUM(fs.SalesQuantity) AS Net_Sold_Quantity
FROM            FactSales AS fs INNER JOIN
                         DimProduct AS dp ON dp.ProductKey = fs.ProductKey INNER JOIN
                         DimDate AS dd ON dd.Datekey = fs.DateKey
GROUP BY fs.ProductKey, dp.ProductName
ORDER BY Net_Revenue DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductKey">
          <DataField>ProductKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Net_Revenue">
          <DataField>Net_Revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Net_Sold_Quantity">
          <DataField>Net_Sold_Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
